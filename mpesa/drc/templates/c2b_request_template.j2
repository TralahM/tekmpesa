<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope
    xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:soap="http://www.4cgroup.co.za/soapauth"
    xmlns:gen="http://www.4cgroup.co.za/genericsoap">
    <soapenv:Header>
        <soap:Token
            xmlns:soap="http://www.4cgroup.co.za/soapauth">{{ token }}</soap:Token>
        <soap:EventID>80049</soap:EventID>
    </soapenv:Header>
    <soapenv:Body>
        <gen:getGenericResult>
            <Request>
                <dataItem>
                    <name>CustomerMSISDN</name>
                    <type>String</type>
                    <value>{{ CustomerMSISDN }}</value>
                </dataItem>
                <dataItem>
                    <name>ServiceProviderCode</name>
                    <type>String</type>
                    <value>{{ serviceprovidercode|default("8337") }}</value>
                </dataItem>
                <dataItem>
                    <name>Currency</name>
                    <type>String</type>
                    <value>{{ currency|default("CDF") }}</value>
                </dataItem>
                <dataItem>
                    <name>Amount</name>
                    <type>String</type>
                    <value>{{ Amount }}</value>
                </dataItem>
                <dataItem>
                    <name>Date</name>
                    <type>String</type>
                    <value>{{ Date }}</value>
                </dataItem>
                <dataItem>
                    <name>ThirdPartyReference</name>
                    <type>String</type>
                    <value>{{ thirdpartyref }}</value>
                </dataItem>
                <dataItem>
                    <name>CommandId</name>
                    <type>String</type>
                    <value>{{ command_id|default("InitTrans_oneForallC2B") }}</value>
                </dataItem>
                <dataItem>
                    <name>Language</name>
                    <type>String</type>
                    <value>{{ language|default("EN") }}</value>
                </dataItem>
                <dataItem>
                    <name>CallBackChannel</name>
                    <type>String</type>
                    <value>{{ callback_channel|default("2") }}</value>
                </dataItem>
                <dataItem>
                    <name>CallBackDestination</name>
                    <type>String</type>
                    <value>{{ callback_url }}</value>
                </dataItem>
                <dataItem>
                    <name>Surname</name>
                    <type>String</type>
                    <value>{{ surname|default("Surname") }}</value>
                </dataItem>
                <dataItem>
                    <name>Initials</name>
                    <type>String</type>
                    <value>{{ initials|default("Initials") }}</value>
                </dataItem>
            </Request>
        </gen:getGenericResult>
    </soapenv:Body>
</soapenv:Envelope>
